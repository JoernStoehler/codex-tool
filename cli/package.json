{
  "name": "@flock/cli",
  "version": "0.0.0",
  "description": "Command-line interface for orchestrating Flock agents.",
  "type": "module",
  "bin": {
    "flock": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch --config tsup.config.ts",
    "build": "tsup src/index.ts --config tsup.config.ts",
    "lint": "eslint .",
    "test": "vitest run",
    "clean": "rm -rf dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "dependencies": {
    "@oclif/core": "^3.27.0",
    "@oclif/plugin-help": "^5.2.14",
    "chalk": "^5.3.0",
    "execa": "^9.3.0"
  },
  "devDependencies": {
    "@types/node": "^22.7.8",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jsdoc": "^48.11.0",
    "eslint-plugin-promise": "^6.6.0",
    "prettier": "^3.3.3",
    "tsup": "^8.3.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.2",
    "vitest": "^2.1.4"
  },
  "oclif": {
    "bin": "flock",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
